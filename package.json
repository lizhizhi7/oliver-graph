{
  "name": "@lizhizhi7/oliver-graph",
  "version": "0.5.9",
  "author": "Oliver Li",
  "main": "dist/index-ts.js",
  "types": "dist/index-ts.d.ts",
  "description": "A workflow manage system with React.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lizhizhi7/oliver-graph/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lizhizhi7/oliver-graph"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "echo \\\"node scripts/tests.js pass\\\"",
    "prepublishOnly": "tsc --p tsconfig_npm.json && gulp",
    "commit": "npx git-cz",
    "lint": "npm run tsc && npm run lint:script && npm run lint:style && npm run lint:md",
    "lint-fix": "npm run lint-fix:script && npm run lint-fix:demo && npm run lint-fix:style",
    "lint-fix:demo": "eslint-tinker ./components/*/demo/*.md",
    "lint-fix:script": "npm run lint:script -- --fix",
    "lint-fix:style": "npm run lint:style -- --fix",
    "lint:md": "remark . -f -q",
    "lint:script": "eslint . --ext '.js,.jsx,.ts,.tsx'",
    "lint:style": "stylelint '{src,demo}/**/*.less' --syntax less",
    "tsc": "tsc --noEmit",
    "github-pkg-login": "npm login --registry=https://npm.pkg.github.com --scope=@lizhizhi7"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "devDependencies": {
    "@types/backbone": "^1.4.3",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "24.0.18",
    "@types/jquery": "^3.5.0",
    "@types/node": "12.7.5",
    "@types/react": "^16.9.0",
    "@types/react-dom": "16.9.0",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.1.0",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-preset-react-app": "^9.1.2",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "css-loader": "3.4.2",
    "cz-conventional-changelog": "^3.2.0",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "3.0.3",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-flowtype": "4.6.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^1.6.1",
    "eslint-plugin-unicorn": "^19.0.1",
    "eslint-tinker": "^0.5.0",
    "fibers": "^4.0.3",
    "file-loader": "4.3.0",
    "fs-extra": "^8.1.0",
    "gulp": "^4.0.2",
    "html-webpack-plugin": "4.0.0-beta.11",
    "husky": "^4.2.5",
    "jest": "24.9.0",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-resolve": "24.9.0",
    "jest-watch-typeahead": "0.4.2",
    "less": "^3.11.3",
    "less-loader": "^6.2.0",
    "mini-css-extract-plugin": "0.9.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.6.4",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.1",
    "preview": "^0.1.3",
    "react": "^16.9.0",
    "react-dev-utils": "^10.2.1",
    "react-dom": "^16.9.0",
    "react-test-renderer": "^16.13.1",
    "resolve": "1.15.0",
    "resolve-url-loader": "3.1.1",
    "sass-loader": "8.0.2",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "2.3.5",
    "typescript": "^3.9.6",
    "url-loader": "2.3.0",
    "webpack": "4.42.0",
    "webpack-dev-server": "3.10.3",
    "webpack-manifest-plugin": "2.2.0",
    "workbox-webpack-plugin": "4.3.1"
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.0",
    "@babel/core": "7.9.0",
    "@svgr/webpack": "4.3.3",
    "antd": "^4.0.0",
    "camelcase": "^5.3.1",
    "dayjs": "^1.8.18",
    "identity-obj-proxy": "3.0.0",
    "immutability-helper": "^3.1.1",
    "jointjs": "^3.2.0",
    "lodash": "^4.17.19",
    "rc-queue-anim": "^1.8.5",
    "react-app-polyfill": "^1.0.6",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "semver": "6.3.0",
    "ts-pnp": "1.1.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  }
}
